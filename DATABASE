CREATE TABLE Credentials(
    BUBBLE_NAME text PRIMARY KEY,
    GAMES_NAMES text,
    BROADCASTER_NAMES text, 
    ACCOUNT_EMAIL text,
    EMAIL_PASSWORD text,
    YOUTUBE_ACCOUNT_PASSWORD text,
    BROWSER_PROFILE_PATH text
    );

    INSERT INTO Credentials (BUBBLE_NAME, GAMES_NAMES, ACCOUNT_EMAIL, EMAIL_PASSWORD, YOUTUBE_ACCOUNT_PASSWORD, BROWSER_PROFILE_PATH)
    VALUES ('Valorant', 'Valorant', '', '', '', '');

    
